{
  "manifest_version": 2,
  "name": "Jobcan Assistant",
  "version": "1.0",
  "description": "This extension will help inputing mon-hour costs in Jobcan ",
  "icons": {
    "128": "img/jobcan.png"
  },

  "permissions": ["storage"],
  "content_scripts": [{
    "matches": ["https://ssl.jobcan.jp/employee/man-hour-manage*"],
    "js": ["js/jquery-2.1.3.min.js", "js/script.js"],
    "css": ["css/style.css"]
  }]
}
